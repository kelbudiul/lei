fn int process_array(arr: int[]) {
    arr[0] = arr[0] + 30;
    arr[1] = 0;
    arr[1] = 75;
    return 0;
}

fn int main() {
    var dynamic: int[] = malloc(5 * sizeof(int));
    dynamic[0] = 5;
    process_array(dynamic);
    print(dynamic[0]);  // Should print 15
    print(dynamic[1]);
    free(dynamic);
    return 0;
}
